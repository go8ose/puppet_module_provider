#!/usr/bin/make -f

OWNER=root
GROUP=root
PREFIX=debian/tmp

binary-indep: build install
binary-arch: build install
binary: build install

clean:
	dh_testdir
	dh_testroot
	rm -f build-stamp configure-stamp
	dh_clean build-stamp

build:
install:
	dh_testdir
	dh_testroot
	dh_installchangelogs
	dh_installdocs
	dh_compress
	dh_installdeb
	dh_gencontrol
	dh_installdirs

	dh_install
	dh_md5sums
	dh_builddeb

.PHONY: build clean install binary-indep
